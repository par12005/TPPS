                $db_id = $fasta['db']['db_id'];

                $run_args = array(
                  'importer_class' => $class,
                  'file_remote' => $file_remote,
                  'analysis_id' => $analysis_id,
                  'seqtype' => $seqtype,
                  'organism_id' => $organism_id,
                  'method' => '2',
                  'match_type' => '0',
                  're_name' => '',
                  're_uname' => '',
                  're_accession' => $re_accession,
                  'db_id' => $db_id,
                  'rel_type' => '',
                  're_subject' => '',
